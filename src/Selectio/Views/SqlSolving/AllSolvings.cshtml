@model IEnumerable<Selectio.Models.SqlSolving>

@{
    ViewData["Title"] = "AllSOlvings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>AllSOlvings</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsCorrect)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SolvedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Solving)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SqlTaskId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsCorrect)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SolvedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Solving)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SqlTaskId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
